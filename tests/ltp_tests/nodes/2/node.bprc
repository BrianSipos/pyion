1
a scheme ipn 'ipnfw' 'ipnadminep'

# ============================================================
# === Define endpoints
# ============================================================

# Default
a endpoint ipn:2.0 x

# Generic for tx/rx 
a endpoint ipn:2.1 x

# Generic for tx/rx      
a endpoint ipn:2.2 x

# CFDP requires it     
a endpoint ipn:2.64 x
a endpoint ipn:2.65 x

# Default port where bpecho is listening    
a endpoint ipn:2.100 x

# Default port where bpreceivefile is listening
a endpoint ipn:2.101 x

# Default port where bpsink is listening
a endpoint ipn:2.102 x

# ============================================================
# === Define outducts/inducts
# ============================================================

a protocol ltp 1400 100
a induct ltp 2 ltpcli
a outduct ltp 1 ltpclo
a outduct ltp 2 ltpclo
r 'ipnadmin node.ipnrc'
w 1
s
